! green
*.color2:       #97cc04
*.color10:      #97cc04

! yellow
*.color3:       #ffad05
*.color11:      #ffad05

! blue
*.color4:       #2d7dd2
*.color12:      #2d7dd2

! magenta
*.color5:       #f230aa
*.color13:      #f230aa

! cyan
*.color6:       #5fbff9
